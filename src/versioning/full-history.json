[
  {
    "version": "v2.0.0",
    "date": "2025-09-26",
    "type": "major",
    "features": [
      "Project renamed from al-progress-wizard to Marksy",
      "Responsive experience improvements (mobile header/footer consolidation)",
      "Subject management enhancements (official A/L list integration, improved search)",
      "Historical version generation tooling (script for 0.0.1 ➜ 2.0.0 placeholders)",
      "Branding & licensing cleanup"
    ],
    "bugfixes": [
      "Existing subjects now consistently appear in search dialog",
      "Removed decorative wave causing overflow on very small screens"
    ],
    "improvements": [
      "Unified app naming & semantic version alignment",
      "ARIA + keyboard accessibility across navigation",
      "Consistent touch target sizing (action buttons)",
      "Clearer version history with LIVE badge on latest"
    ]
  },
